# Example environment variables
DATABASE_URL=postgresql://postgres:password@db:5432/postgres
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/1
CELERY_RESULT_BACKEND=redis://redis:6379/2
PGVECTOR_DIM=1536
SECRET_KEY=replace-me-with-secure-key

# Grok AI (embedding) configuration
# Example: https://api.grok.ai/v1/embeddings
GROK_API_URL=https://api.grok.ai/v1/embeddings
# Your Grok API key
GROK_API_KEY=replace-with-grok-api-key
# Optional: model name to request for embeddings
GROK_EMBEDDING_MODEL=grok-embedding-1
